{
    "name": "softworx/rocxolid",
    "description": "Laravel CRUD & components library and extension",
    "keywords": [
        "laravel",
        "crud"
    ],
    "type": "library",
    "authors": [{
        "name": "softworx",
        "email": "hello@softworx.digital"
    }],
	"license": "MIT",
    "require": {
        "php": "~7.4",
        "ext-intl" : "*",
        "ext-soap" : "*",
        "ext-bcmath" : "*",
        "ext-gd" : "*",
        "ext-openssl" : "*",
        "ext-tidy" : "*",
        "laravel/framework": "^8.0",
        "laravel/ui": "^3.3",
        "laravelcollective/html": "^5.8|^6.0",
        "doctrine/annotations": "1.11.1",
        "doctrine/dbal": "^2.9",
        "intervention/image": "^2.5",
        "barryvdh/laravel-translation-manager": "^0.5",
        "staudenmeir/eloquent-has-many-deep": "^1.11",
        "spipu/html2pdf": "^5.2"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2"
    },
    "autoload": {
        "psr-4": {
            "Softworx\\RocXolid\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Softworx\\RocXolid\\ServiceProvider"
            ]
        },
        "rocXolid": {
            "title": {
                "en": "rocXolid",
                "sk": "rocXolid"
            }
        }
    }
}
